pipeline {
  agent any

  stages {
    stage('Hello') {
      steps {
        echo 'Hello Jenkins!'
      }
    }
    stage('Build') {
      steps {
        echo 'Building'
      }
    }
    stage('Test') {
      steps {
        echo 'Testing'
      }
    }
    stage('Release') {
      steps {
        echo 'Releasing'
      }
    }
  }
}